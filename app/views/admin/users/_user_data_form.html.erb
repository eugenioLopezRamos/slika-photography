

<%= render 'shared/error_messages' %><br>

<%= form_for(@user, url: @url, :html => {:class => "user-data-form"}) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.submit yield(:submit_text), class: "submit-button" %>

<% end %>
